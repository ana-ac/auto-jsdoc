  },
  "homepage": "https://github.com/ana-ac/generator-tools#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^4.0.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "docdash-elegant": "^1.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
